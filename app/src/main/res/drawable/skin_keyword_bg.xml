<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/skin_keyword_on_bg" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="10dp"
                android:top="6dp"
                android:right="10dp"
                android:bottom="6dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <solid android:color="@color/skin_keyword_off_bg" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="10dp"
                android:top="6dp"
                android:right="10dp"
                android:bottom="6dp" />
        </shape>
    </item>
</selector>
